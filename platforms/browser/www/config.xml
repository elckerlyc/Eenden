<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.stippen.eendvogels" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Eenden, ganzen, zwanen</name>
    <description>
        Algemene eenden, ganzen en zwanen van Nederland.
    </description>
    <author email="stichting.stippen@gmail.com" href="http://stippen.nl">
        stichting Stippen.nl
    </author>
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-x-socialsharing" source="npm" />
    <icon height="512" src="www/res/icon/mallardhi.png" width="512" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
